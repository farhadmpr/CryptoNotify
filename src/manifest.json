{
  "name": "CryptoNotify",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CryptoNotify",
    "default_popup": "popup.html"
  },
  "permissions": ["notifications"]
}
